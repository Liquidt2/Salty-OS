# ═══════════════════════════════════════════
# BKE LOGISTICS — Environment Config
# Copy to .env and fill in your keys
# ═══════════════════════════════════════════

# PostgreSQL
PG_ADMIN_USER=postgres
PG_ADMIN_PASSWORD=bke_master_2025

# Salty OS
SALTY_PORT=3000
API_PORT=3001
SALTY_AUTH_TOKEN=

# Agent Zero LLM
AZ_CHAT_PROVIDER=openai
AZ_CHAT_MODEL=gpt-4o
AZ_CHAT_API_KEY=sk-your-key-here
AZ_UTIL_PROVIDER=openai
AZ_UTIL_MODEL=gpt-4o-mini
AZ_UTIL_API_KEY=sk-your-key-here
AZ_EMBED_PROVIDER=openai
AZ_EMBED_MODEL=text-embedding-3-small
AZ_EMBED_API_KEY=sk-your-key-here

# n8n
N8N_ENCRYPTION_KEY=change-this-to-a-random-string

# Postiz
POSTIZ_JWT_SECRET=change-this-postiz-secret

# Timezone
TZ=America/Chicago
